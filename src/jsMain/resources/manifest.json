{
  "manifest_version": {MANIFEST_VERSION},
  "name": "Crossword Scraper",
  "description": "Download digital puzzle files from crossword applets for offline solving.",
  "version": "1.3.32",
  "icons": {
    "128": "128.png"
  },
  "{ACTION_KEY}": {
    "default_icon": "48.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "activeTab",
    "cookies",
    "storage",
    "webNavigation"
    {PERMISSIONS}
  ],
  "{OPTIONAL_HOST_PERMISSION_KEY}": [
    "http://*/",
    "https://*/"
  ],
  {BROWSER_SPECIFIC_SETTINGS}
}
